const AsapQueue_js_1 = require("./AsapQueue.js");
const TaskFactory_js_1 = require("./TaskFactory.js");
const asapQueue = new AsapQueue_js_1.AsapQueue();
const taskFactory = new TaskFactory_js_1.TaskFactory(asapQueue.registerPendingError);
/**
 * Calls a task as soon as possible after returning, in its own event, with priority
 * over other events like animation, reflow, and repaint. An error thrown from an
 * event will not interrupt, nor even substantially slow down the processing of
 * other events, but will be rather postponed to a lower priority event.
 * @param {{call}} task A callable object, typically a function that takes no
 * arguments.
 */ function asap(task) {
    asapQueue.enqueueTask(taskFactory.create(task));
}
